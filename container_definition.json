[{
    "name": "ecsEc2App",
    "image": "210734875628.dkr.ecr.eu-central-1.amazonaws.com/ecs-practical-app:latest",
    "essential" : true,
    "entryPoint": [],
    "command": [],
    "environment": [],
    "mountPoints": [],
    "volumesFrom": [],
    "logConfiguration" : {
        "logDriver" : "awslogs",
        "options" : {
            "awslogs-group" : "/ec2/service/",
            "awslogs-region" : "eu-central-1",
            "awslogs-stream-prefix" : "ecs"
        }
    },
    "portMappings": [
        { 
            "containerPort": 5000, 
            "hostPort": 5000, 
            "protocol": "tcp" 
        }
    ],
    "cpu": 256,
    "memory" : 768,
    "memoryReservation": 768,
    "linuxParameters": {
        "maxSwap": 512,
        "swappiness": 50
    }
  }]